<p id="notice"><%= notice %></p>

<h1>Listing Bio Urls</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bio_urls.each do |bio_url| %>
      <tr>
        <td><%= link_to 'Show', bio_url %></td>
        <td><%= link_to 'Edit', edit_bio_url_path(bio_url) %></td>
        <td><%= link_to 'Destroy', bio_url, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bio url', new_bio_url_path %>
